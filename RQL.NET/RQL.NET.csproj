<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
   <TargetFrameworks>netstandard2.0;</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>RQL.NET</PackageId>
    <authors>Ashton Kinslow</authors>
    <owners>ashtonian</owners>
    <Description>RQL.NET is a resource query language for .NET intended for use with REST apps. It provides a simple, hackable api for creating dynamic sql queries from user submitted JSON.</Description>
    <PackageVersion>0.0.12-beta</PackageVersion>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>web utility json url rql sql query dynamic builder data-access parser </PackageTags>
    <!-- <PackageLicenseUrl>https://github.com/Ashtonian/RQL.NET</PackageLicenseUrl> -->
    <PackageProjectUrl>https://github.com/Ashtonian/RQL.NET</PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2"/>
  </ItemGroup>
</Project>